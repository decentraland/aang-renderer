                <ui:Label text="&lt;font-weight=600&gt;Welcome to Decentraland!" name="Welcome" class="enter-name-window__welcome" />
                <ui:TextField label="Enter your username" placeholder-text="Any name you want" name="UsernameField" max-length="15" hide-placeholder-on-focus="true" class="dcl-text-field enter-name-window__text-field" />
                <ui:TextField label="Email (optional)" placeholder-text="We won&apos;t send you spam" name="EmailField" hide-placeholder-on-focus="true" keyboard-type="EmailAddress" class="dcl-text-field enter-name-window__text-field" />
