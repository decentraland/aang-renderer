                            <ui:VisualElement class="popup-color__preset" />
                            <ui:VisualElement class="popup-color__preset" />
                        </ui:VisualElement>
                        <ui:VisualElement name="CloseButton" class="dcl-dropdown-popup__close dcl-clickable" />
                    </ui:VisualElement>
                </UI.Elements.DCLDropdownElement>
            </ui:VisualElement>
        </ui:VisualElement>
        <ui:VisualElement name="Confirmation" class="confirmation" style="display: none;">
            <ui:Label text="&lt;font-weight=600&gt;UserName is Ready to Jump In!" name="Title" class="confirmation__title" />
            <ui:VisualElement name="CharacterArea" class="confirmation__character-area" />
            <ui:VisualElement name="ButtonsContainer" class="confirmation__buttons">
