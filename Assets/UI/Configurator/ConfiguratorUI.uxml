    <Style src="project://database/Assets/UI/Configurator/ConfiguratorStyle.uss?fileID=7433441132597879392&amp;guid=83de1c6279f824cdcbe4149342d94f6f&amp;type=3#ConfiguratorStyle" />
    <ui:VisualElement name="root" class="root">
        <ui:VisualElement picking-mode="Ignore" name="Loader" class="loader" style="display: none;">
            <ui:VisualElement picking-mode="Ignore" name="Icon" class="loader__icon" />
        </ui:VisualElement>
        <ui:VisualElement name="Container" picking-mode="Ignore" class="container container--customize">
            <ui:VisualElement name="EnterNameWindow" class="container__body enter-name-window">
